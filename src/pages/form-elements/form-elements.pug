extends  /layout/ui-kit/ui-kit
include /components/description/description
include /components/field/field
include /components/dropdown/dropdown
include /components/dropdown-date/dropdown-date
include /components/checkbox/checkbox
include /components/radio/radio
include /components/toggle/toggle
include /components/like-button/like-button
include /components/rate button/rate button
include /components/range slider/range slider
include /components/buttons/buttons
include /components/pagination/pagination
include /components/expandable-checkbox/expandable-checkbox
include /components/bullet-list/bullet-list
include /components/icon-list/icon-list
include /components/comment/comment


block head
  title Form elements

block body
  - const {settingsTextField, descriptions, settingsDropdown,checkboxSettings,} = require('./form-elements.json')
  - const {radioSettings,toggleSettings, likeButtonSettings, rangeSlider, buttonSettings} = require('./form-elements.json')
  - const {dropdownDateSettings,expandableCheckbox,richCheckboxSettings,comment} = require('./form-elements.json')
    .ui-kit__form-elements.form-elements
      .form-elements__container
        .form-elements__content.form-elements__content_row.form-elements__content_margin-b-100
          .form-elements__content.form-elements__content_column.form-elements__content_margin-r-53.form-elements__content_width-320

            .form-elements__block.form-elements__block_column.form-elements__block_margin-b-22
              .form-elements__description
                +description(descriptions.one)
              .form-elements__text-field
                +textField(settingsTextField.text)

            .form-elements__block.form-elements__block_column.form-elements__block_margin-b-20
              .form-elements__description
                +description(descriptions.two)
              .form-elements__text-field
                +textField(settingsTextField.hover)

            .form-elements__block.form-elements__block_column.form-elements__block_margin-b-22
              .form-elements__description
                +description(descriptions.three)
              .form-elements__dropdown
                +dropdown(settingsDropdown.guestsOne)

            .form-elements__block.form-elements__block_column.form-elements__block_margin-b-42
              .form-elements__description
                +description(descriptions.four)
              .form-elements__text-field
                +textField(settingsTextField.mask)

            .form-elements__block.form-elements__block_column.form-elements__block_margin-b-42
              .form-elements__dropdown-date
                +dropdownDate(dropdownDateSettings.range)

            .form-elements__block.form-elements__block_column.form-elements__block_margin-b-42.form-elements__block_size-266
              .form-elements__dropdown-date
                +dropdownDate(dropdownDateSettings.filter).form-elements__dropdown

            .form-elements__block.form-elements__block_column.form-elements__block_size-266
              .form-elements__description
                +description(descriptions.five)
              .form-elements__text-field
                +textField(settingsTextField.subscribe)

          .form-elements__content.form-elements__content_column.form-elements__content_width-320

            .form-elements__block.form-elements__block_column.form-elements__block_margin-b-41.form-elements__block_size-266
              .form-elements__description
                +description(descriptions.six)
              .form-elements__checkbox
                +checkbox(checkboxSettings.smoking)
                +checkbox(checkboxSettings.pets)
                +checkbox(checkboxSettings.guests)

            .form-elements__block.form-elements__block_column.form-elements__block_margin-b-41
              .form-elements__description.form-elements__description_margin-b-16
                +description(descriptions.seven)
              .form-elements__radio
                +radio(radioSettings.male)
                +radio(radioSettings.female)

            .form-elements__block.form-elements__block_column.form-elements__block_margin-b-31
              .form-elements__description.form-elements__description_margin-b-16
                +description(descriptions.eight)
              .form-elements__toggle
                +toggle(toggleSettings.first)
                +toggle(toggleSettings.second)

            .form-elements__block.form-elements__block_column.form-elements__block_margin-b-41
              .form-elements__description.form-elements__description_margin-b-16
                +description(descriptions.nine)
              .form-elements__like-buttons
                +like_button(likeButtonSettings.first)
                +like_button(likeButtonSettings.second)
            .form-elements__block.form-elements__block_column
              +description(descriptions.ten)
              .form-elements__block_row
                .form-elements__block.form-elements__block_margin-r-20
                  +rateButton(4).form-elements__rate-button
                +rateButton(5).form-element__rate-button
          .form-elements__content.form-elements__content_column.form-elements__content_width-320
            .form-elements__block.form-elements__block_margin-b-20.form-elements__block_size-266
              +range_slider(rangeSlider)
            .form-elements__block.form-elements__block_column.form-elements__block_margin-t-10.form-elements__block_margin-b-41
              +description(descriptions.eleven)
              .form-elements__block.form-elements__block_row.form-elements__block_margin-b-32
                .form-elements__block_margin-r-20
                  +button(buttonSettings.one)
                +button(buttonSettings.two)
              .form-elements__block.form-elements__block_row.form-elements__block_margin-b-32
                .form-elements__block_margin-r-20
                  +button(buttonSettings.three)
                +button(buttonSettings.four)
              .form-elements__block.form-elements__block_row.form-elements__block_margin-b-32
                .form-elements__block_margin-r-20
                  +button(buttonSettings.five)
                +button(buttonSettings.six)
              .form-elements__block
                +button(buttonSettings.seven)
            .form-elements__block.form-elements__block_column
              +description(descriptions.twelve)
              +pagination
        .form-elements__content.form-elements__content_row.form-elements__content_margin-b-95
          .form-elements__content.form-elements__content_column.form-elements__content_margin-r-126.form-elements__content_width-320
            .form-elements__block.form-elements__block_column
              .form-elements__block.form-elements__block_column.form-elements__block_size-266
                +description(descriptions.thirteen)
                .form-elements__block.form-elements__block_margin-b-20
                  +dropdown(settingsDropdown.room)
              .form-elements__block.form-elements__block_column.form-elements__block_size-266
                +description(descriptions.fourteen)
                .form-elements__block.form-elements__block_margin-b-20
                  +dropdown(settingsDropdown.roomActive)
              .form-elements__block.form-elements__block_column.form-elements__block_size-266.form-elements__block_margin-t-144 
                .form-elements__block.form-elements__block_margin-b-20
                  +expandable-checkbox(expandableCheckbox.close)
                +expandable-checkbox(expandableCheckbox.open)
          .form-elements__content.form-elements__content_column
            .form-elements__content.form-elements__content_row.form-elements__content_margin-b-100
              .form-elements__content.form-elements__content_column.form-elements__content_width-320.form-elements__content_margin-r-72
                +description(descriptions.fifteen)
                +dropdown(settingsDropdown.guestsTwo)
              .form-elements__content.form-elements__content_column.form-elements__content_width-320
                +description(descriptions.fifteen)
                +dropdown(settingsDropdown.guestsThree)
            .form-elements__content_row.form-elements__content_margin-top-150
              .form-elements__content_column.form-elements__content_width-320
                .form-elements__block.form-elements__block_column.form-elements__block_margin-t-10.form-elements__block_size-266
                  +description(descriptions.sixteen)
                  +checkbox(richCheckboxSettings.corridor)
                  .form-elements__block.form-elements__block_margin-t-10
                    +checkbox(richCheckboxSettings.assistant)
              .form-elements__content.form-elements__content_column.form-elements__content_width-320
                .form-elements__block.form-elements__block_margin-t-10.form-elements__block_column
                  +description(descriptions.seventeen)
                  +bulletList({"descriptionText":"bullet list"})
        .form-elements__content.form-elements__content_row
          .form-elements__content.form-elements__content_column.form-elements__content_width-320
            .form-elements__block.form-elements__block_column.form-elements__block_size-266
              .form-elements__block.form-elements__block_margin-b-20
                +iconList({"typeIcon":'insert_emoticon',
                  "header":'Комфорт',
                  "text":'Шумопоглощающие стены'})
              +iconList({"typeIcon":'location_city',
                "header":'Удобство',
                "text":'Окно в каждой из спален'})
          .form-elements__content.form-elements__content_column.form-elements__content_width-711
            .form-elements__block.form-elements__block_margin-t-5
              +comment(comment)
