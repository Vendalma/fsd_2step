{
  "rules": {
    "no-unused-vars": "off"
  },
  "env": {
    "es6": true,
    "browser": true,
    "jquery": true,
    "node": true
  },
  "parserOptions": {
    "ecmaVersion": 2018,
    "sourceType": "module"
  },
  "plugins": [
    "fsd"
  ],
  "extends": [
    "eslint:recommended",
    "plugin:fsd/all"
  ]
}